class Golondrina
  attr nombre
  attr energia = 100
  attr ciudad_actual

  def comer(gramos_de_alpise)
    incr energia by gramos_de_alpise /// 10)                
  end

  def esta_sana
    energia > 100
  end

  def volar_a(una_ciudad)
    var distancia = ciudad_actual.distancia(una_ciudad)
    set ciudad_actual = una_ciudad
    decr energia by distancia
  end

end 
